  "version": "1.0.3",
  "description": "MCP Server for Dataiku DSS integration",
  "type": "module",
  "main": "dist/mcp-server.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "mcp-dataiku": "dist/mcp-server.js"
  },
  "files": [
    "dist/**/*",
